{
  "name": "lukaschel/pimcore-bundle-configuration",
  "description": "Individual pimcore bundle configurations for sites and languages",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Lukaschel",
      "email": "lukaschel33@gmail.com"
    },
    {
      "name": "Michael Frank",
      "email": "contact@phpdoctor.cc",
      "homepage": "https://www.the-spirit-of-IT.com/",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Lukaschel\\PimcoreConfigurationBundle\\": ""
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Lukaschel\\PimcoreConfigurationBundle\\PimcoreConfigurationBundle"
      ]
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "composer/installers": "^1.9 || ^2.0",
    "pimcore/pimcore": "^6.9 || ^10.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  }
}
